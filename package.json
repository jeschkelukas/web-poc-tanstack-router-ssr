  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/generator": "^7.23.6",
    "@rollup/plugin-babel": "^6.0.4",
    "@types/express": "^4.17.21",
    "@types/jsesc": "^3.0.3",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "isbot": "^4.3.0",
